<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://ocrsdk.com/schema/taskDescription-1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ocrsdk.com/schema/taskDescription-1.0.xsd http://ocrsdk.com/schema/taskDescription-1.0.xsd">

  <fieldTemplates>
      <text id="basicTemplate" left="-1" top="-1" right="-1" bottom="-1">
                  <language>English</language>
                  <textType>normal</textType>
                  <oneTextLine>false</oneTextLine>
      </text>
  </fieldTemplates>
  <page applyTo="0">
          <!-- registration number -->
          <text template="basicTemplate" id="registrationNumber" left="0" top="0" right="0" bottom="0">
               <oneTextLine>true</oneTextLine>
          </text>
         <!-- circle -->
          <text template="basicTemplate" id="circle" left="0" top="0" right="0" bottom="0">
          </text>
          <!-- registered keeper -->
          <text template="basicTemplate" id="registeredKeeper" left="0" top="0" right="0" bottom="0">
          </text>
          <!-- document reference numer -->
          <text template="basicTemplate" id="referenceNumber" left="0" top="0" right="0" bottom="0">
          </text>
          <!-- previous registere keeper -->
          <text template="basicTemplate" id="previousKeeper" left="0" top="0" right="0" bottom="0">
          </text>
          <!-- special notes -->
          <text template="basicTemplate" id="specialNotes" left="0" top="0" right="0" bottom="0">
          </text>
  </page>
</document>
